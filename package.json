{
  "name": "slideshow",
  "author": "Akronat <akronat@does.not.exist.com>",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "main": "public/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "preebuild": "npm run build",
    "ebuild": "electron-builder -wl -c.extraMetadata.main=build/electron.js -c.extraMetadata.homepage=./"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "myappidprobably",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "linux": {
      "category": "Media",
      "target": [
        "AppImage"
      ],
      "icon": "public/logo512.png"
    },
    "win": {
      "target": "portable",
      "icon": "public/logo512.png"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "classnames": "^2.2.6",
    "electron-is-dev": "^1.2.0",
    "react": "^16.8.6",
    "react-autobind": "^1.0.6",
    "react-dom": "^16.8.6",
    "react-hammerjs": "^1.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/classnames": "^2.2.10",
    "@types/filesystem": "^0.0.29",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.19.1",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-hammerjs": "^1.0.1",
    "electron": "^10.1.4",
    "electron-builder": "^22.9.1",
    "react-scripts": "^4.0.0",
    "typescript": "^4.0.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
